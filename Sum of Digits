import java.util.*;
public class Alice
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int digit;
        int sum=0;
        digit=n%10;
        sum=sum+digit;
        n=n/10;
        digit=n%10;//second iteration
        sum=sum+digit;
        n=n/10;
        System.out.println(sum);
    }
}
