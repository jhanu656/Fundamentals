import java.util.*;
public class Series5
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=7;
        for (int i=1;i<=a;i++)
        {
            
            b=b+4;
            System.out.print(b*b+" ");
        }
    }
}
