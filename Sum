import java.util.*;
public class Sum
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int s=0;
        int a=sc.nextInt();
        while(a!=0)
        {   
            s=s+(a%10);
            a=a/10;
        }
        System.out.println(s);
    }
}
