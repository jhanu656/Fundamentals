import java.util.*;
public class Friendly
{
	public static void main(String[] args)
	{
		
		Scanner sc = new Scanner(System.in);
		int a= sc.nextInt();
		int b= sc.nextInt();
		int c=0,d=0;
		for(int i=1;i<a;i++)
		{
			if(a%i==0)
			{
				c=c+i;
			}
		}
		for(int i=1;i<b;i++)a
		{
			if(b%i==0)
				{
					d=d+i;
				}
		}
		if(c==a && d==b)
		{
			System.out.print("Friendly Pair");
		}
		else
		{
			System.out.print("Not Friendly Pair");
		}
	}	
}
