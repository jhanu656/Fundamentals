import java.util.*;
public class Reverse
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
         int sum=0;
        while (a>0)
        {
            sum=(sum*10)+(a%10);
            a=a/10;
        }
        System.out.println(sum);
    }
}
