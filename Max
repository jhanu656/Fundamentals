import java.util.*;
public class Max
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int n=0;
        int b[]=new int[a];
        for (int i=0;i<a;i++)
            {
                b[i]=sc.nextInt();
                if(b[i]>n)
                    {
                        n=b[i];
                    }
            }
        System.out.println(n);
    }
}
