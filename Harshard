import java.util.*;
public class Harshard
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int sum=0;
        for (int i=1;i<=a;i++)
        {
            sum=sum+(a%10);
            a=a/10;
        }
        if ((a%sum)==0)
        System.out.println("Harshad Number");
        else
        System.out.println("Not Harshad Number");
    }
}
