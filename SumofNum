import java.util.*;
public class Sumofnum
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int s=0;
        for (int i=a;i<=b;i++)
        {
            
            s=s+i;
            if (i==b)
            break;
        }
        System.out.println(s);
    }
}
