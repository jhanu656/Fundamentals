import java.util.*;
public class Sumarray
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int sum=0;
        int a=sc.nextInt();
        int b[]=new int[a];
        for (int i=0;i<a;i++)
        {
            b[i]=sc.nextInt();
        }
        for (int j=0;j<a;j++)
        {
        sum=sum+b[j];
        }
        System.out.println(sum);
    }
}
