import java.util.*;
public class String4
{
    public static void main(String[]args)
    {
        Scanner sc=new  Scanner(System.in);
        String a=sc.next();
        char j;
        String s="";
        for (int i=a.length()-1;i>=0;i--)
        {
            j=a.charAt(i);
            s=s+j;
        }
        System.out.println(s);
    }
}
