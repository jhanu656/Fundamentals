import java.util.*;
public class Series4
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        double b=0.5;
        for (int i=1;i<=a;i++)
        {
            System.out.print(b+" ");
            b=b*3;
        }
    }
}
