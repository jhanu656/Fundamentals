import java.util.*;
public class Series1
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a number:");
        int n=sc.nextInt();
        int i;
        for(i=1;i<=n;i++){
        System.out.print((i*i+" "));
        }
    }
}
