import java.util.*;
public class Factorial
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=1;
        for (int i=1;i<=a;i++)
        {
            b=b*i;
            if (a==b)
            break;
        }
        if (a==b)
        System.out.println("Yes");
        else
        System.out.println("No");
    }
}
