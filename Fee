import java.util.Scanner;
public class Fee
{
    public static void main(String args[])
    {
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter the student type");
    String m=sc.nextLine();
    switch(m)
    {
        case "MSDS":
            System.out.println("Enter tuition fee ");
            float a=sc.nextFloat();
            System.out.println("Enter bus fee ");
            float b=sc.nextFloat();
            System.out.printf("The fees to be paid by the student is Rs.%.2f",(a+b));
            break;
        case "MSH":
            System.out.println("Enter tuition fee ");
            float c=sc.nextFloat();
            System.out.println("Enter hostel fee  ");
            float d=sc.nextFloat();
            System.out.printf("The fees to be paid by the student is Rs.%.2f",(d+c));
            break;
        case "MGSDS":
            System.out.println("Enter tuition fee");
            float e=sc.nextFloat();
            System.out.println("Enter bus Fee");
            float f=sc.nextFloat();
            float h=(150*e)/100;
            System.out.printf("The fees to be paid by the student is Rs.%.2f",(h+f));
            break;
        case "MGSH":
            System.out.println("Enter tuition fee");
            float g=sc.nextFloat();
            System.out.println("Enter hostel fee");
            float i=sc.nextFloat();
            float j=(150*g)/100;
            System.out.printf("The fees to be paid by the student is Rs.%.2f",(j+i));
            break;
}
        
    }
    }
            
            
            
        
